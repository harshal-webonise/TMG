border-radius()
  -webkit-border-radius arguments
  -moz-border-radius arguments
  border-radius arguments

.cycle-slideshow
	border-radius(10px)
	overflow: hidden

.cycle-slideshow .middleContent 
	height: 450px
	width: 1140px
	border-radius(0)

.progressWrap
	height: 15px
	margin: 20px 0
	background: #fff
	border-radius(5px)

.progressBar
	background: #693
	height: 15px
	width: 15%
	border-radius(5px)

.quastionWrap
	height: 165px

.qustWrap
	border-radius(10px)
	background: #fff
	padding: 20px
	position: relative
	b
		position: absolute
		top: 20px
		left: 20px
	p
		padding-left: 30px

.ansWrap
	margin: 0 15px 15px 0
	float: left
	width: 200px
	li 	
		background: #3399cc
		border-radius(10px)
		padding: 0 10px
		height: 30px
		line-height: 30px;		
		color: #fff
		margin-bottom: 10px
	li:first-child
		margin-left: 0
	input
		opacity: 0
		display: inline-block
		height: 23px
		width: 23px
		padding: 0
		margin: 0
		vertical-align: top	
	label
		width: 85%
		padding-left: 5px
		margin: 0

.commentWrap
	float: left
	width: 885px 
	color: #e1e1e1
	font-size: 12px
	textarea
		border-radius(10px)
		height: 140px
		width: 99.5%
		padding: 10px
		margin-bottom: 12px
		background: #e1e1e1
		color: #000

.radioWrap
	background: url(/images/radio.png) left top no-repeat


.radioWrap.checked 
	background-position: left -90px 

.btnWrap
	margin: 20px 0
	.btn
		margin-left: 20px
	.btn:first-child
		margin-left: 0