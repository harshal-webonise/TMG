border-radius()
  -webkit-border-radius arguments
  -moz-border-radius arguments
  border-radius arguments

.cycle-slideshow .middleContent 
	height: 450px
	width: 1140px

.progressWrap
	height: 15px
	margin: 20px 0
	background: #fff
	border-radius(5px)

.progressBar
	background: #693
	height: 15px
	width: 15%
	border-radius(5px)

.quastionWrap
	height: 300px

.qustWrap
	border-radius(10px)
	background: #fff
	padding: 20px

.ansWrap
	li 	
		float: left
		background: #3399cc
		border-radius(10px)
		padding: 0 10px
		height: 30px
		line-height: 30px;
		width: 18.3%
		margin-left: 2%
		color: #fff
	li:first-child
		margin-left: 0
	input
		opacity: 0
		display: inline-block
		height: 23px
		width: 23px
		padding: 0
		margin: 0
		vertical-align: top	
	label
		width: 85%
		padding-left: 5px
		margin: 0

.radioWrap
	background: url(/images/radio.png) left top no-repeat


.radioWrap.checked 
	background-position: left -90px 

.btnWrap
	margin: 20px 0
	.btn
		margin-left: 20px
	.btn:first-child
		margin-left: 0